root = true

[*]
end_of_line = lf
charset = utf-8
insert_final_newline = true

[*.{java, xml, md}]
ij_java_imports_layout = $*, |, *, java.**, javax.**, org.**, org.openqa.selenium.**
ij_java_class_count_to_use_import_on_demand = 50
ij_java_names_count_to_use_import_on_demand = 50
indent_style = space
indent_size = 2
ij_continuation_indent_size = 4
